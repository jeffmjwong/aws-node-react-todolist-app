{
  "name": "node-react-todolist-app",
  "version": "1.0.0",
  "description": "Todolist app built with node and react, deploying to aws",
  "private": true,
  "scripts": {
    "start:dev:client": "webpack-dev-server --config webpack.dev.config.js --port 3000",
    "build": "webpack --config webpack.prod.config.js",
    "prebuild:server": "rm -rf server_build",
    "build:server": "webpack --config webpack.server.config.js",
    "start:dev:server": "build:server"
  },
  "author": "Jeff",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "dotenv": "^8.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "pg-promise": "^9.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  }
}
